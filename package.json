{
  "name": "anna-liisa",
  "version": "1.0.0",
  "description": "anna-liisa",
  "homepage": "https://lauravuo.github.io/anna-liisa",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf build/ && webpack --config ./webpack/prod.js",
    "ci:automerge": "circleci-automerge --filter=\"^greenkeeper/\"",
    "model": "babel-node ./tools/parse-model",
    "start": "webpack-dev-server --open \"Google Chrome\" --config ./webpack/dev.js",
    "storybook": "start-storybook",
    "test": "npm run test:lint && npm run test:unit",
    "test:e2e": "cd e2e && make && nightwatch && make stop",
    "test:ci:e2e": "cd e2e && nightwatch",
    "test:lint": "eslint ./src --format codeframe",
    "test:unit": "jest ./src --coverage",
    "test:update": "jest ./src --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lauravuo/anna-liisa"
  },
  "keywords": [
    "library"
  ],
  "author": "lauravuo",
  "license": "MIT",
  "jest": {
    "testRegex": ".*\\.test\\.js$",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  },
  "dependencies": {
    "@babel/runtime": "7.7.7",
    "connected-react-router": "6.6.1",
    "firebase": "7.6.1",
    "firebaseui": "4.4.0",
    "grommet": "2.9.0",
    "grommet-icons": "^4.4.0",
    "history": "4.10.1",
    "polished": "3.4.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-observable": "1.2.0",
    "rxjs": "6.5.4",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "7.7.7",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@storybook/addon-storyshots": "5.2.8",
    "@storybook/react": "5.2.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "chromedriver": "79.0.0",
    "circleci-automerge": "1.1.0",
    "config": "3.2.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "jest-svg-transformer": "1.0.0",
    "nightwatch": "1.3.2",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.0",
    "svg-url-loader": "3.0.3",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-merge": "4.2.2",
    "webpack-version-file": "0.1.5"
  }
